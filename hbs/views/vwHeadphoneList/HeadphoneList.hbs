	<form action="#" class="search-location col-md-9 mt-5" style="margin: 0 auto">
		<div class="row justify-content-center">
			<div class="col-lg-10 align-items-end ">
				<div class="form-group">
					<div class="form-field " >
						<input type="text" class="form-control " style="border-style:ridge;" placeholder="Search item">
						<button><span class="ion-ios-search"></span></button>
				  	</div>
			  	</div>
			</div>		
		</div>
		<div >
			<a class="btn btn-outline-success" href="/admin/mobile/addMobile">
				<i class="fa fa-plus" aria-hidden="true"></i>	
				Add new Headphone
			</a>
		</div>
	</form>
	<section class="ftco-section goto-here">
    	<div class="container">
			{{#if this.empty}}
			<div class="card-body">
				<p class="card-text">Không có dữ liệu</p>
			</div>
			{{else}}
			<table class="table table-striped" >
  				<tbody>
    				{{#each this.mobile}}
    				<tr>
      					<th scope="row"> 
						<td>
							<div class="col-md-5">
        						<div class="property-wrap ftco-animate">
        							<a href="#" class="img" style="background-image: url(images/work-1.jpg);"></a>
        							<div class="text"  >
        								<h3><a href="#">{{Name}}</a></h3>
        								<span class="location">Giá hiện tại: {{Price}}</span><br>
										<span class="location">Ngày đăng: {{Date}}</span> <br>
										<span class="location">Số lượt ra giá: {{Turn}}</span> <br>
										<span class="location">Bidder đấu giá cao nhất: {{BidderId}}</span> <br>
										<span class="location">Thời gian còn lại: {{TimeRemain}}</span> <br>
        								<a href="#" class="d-flex align-items-center justify-content-center btn-custom">
        									<span class="glyphicon glyphicon-heart">&#10084;</span> 
        								</a>
        							</div>
									<a href="/admin/mobile/editMobile/{{ID}}">
										<img src="https://img.icons8.com/cotton/2x/edit.png" width="100" height="100">
									</a>		
        						</div>
        					</div>
						</td>
	  					</th>
    				</tr>
					{{/each}}
  				</tbody>
			</table>
			{{/if}}
       	</div>
